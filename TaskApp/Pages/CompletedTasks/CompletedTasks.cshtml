@page
@model TaskApp.Pages.CompletedTasks.CompletedTasksModel
@{
}
<form method="post">
    <table border="1" width="100%">
        <thead>
            <tr>
                <th>Task Numarası</th>
                <th>Konu</th>
                <th>Açıklana</th>
                <th>Tamamlanma Tarih</th>
                
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Tickets.Count; i++)
            {
            <tr>
                <td>
                <label>@Model.Tickets[i].Id</label>
                </td>
                <td>
                    <label>@Model.Tickets[i].Subject</label>
                </td>
                <td>
                    <label>@Model.Tickets[i].Description</label>
                </td>
              
                <td>
                    <label>@Model.Tickets[i].CompletedTime</label>
                </td>
              

            </tr>
            }

        </tbody>
    </table>
</form>